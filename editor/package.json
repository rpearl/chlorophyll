{
  "name": "chlorophyll",
  "productName": "chlorophyll",
  "version": "0.0.0",
  "contributors": [
    "Ryan Pearl <rpearl@endofunctor.org>",
    "Chris Williamson <c.williamso@gmail.com>"
  ],
  "description": "Tools for photon synthesis",
  "keywords": [
    "leds",
    "art",
    "rainbows",
    "visual",
    "vj",
    "modular_synthesis"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rpearl/chlorophyll.git"
  },
  "bugs": {
    "url": "https://github.com/rpearl/chlorophyll/issues"
  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "build:all": "node config/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node config/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node config/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node config/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node config/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node config/build.js",
    "build:web": "cross-env BUILD_TARGET=web node config/build.js",
    "build": "cross-env BUILD_TARGET=$(uname -s | tr '[:upper:]' '[:lower:]') node config/build.js",
    "dev": "node config/dev-runner.js",
    "test": "jest",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src src/common",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.renderer.config.js",
    "install": "electron-rebuild",
    "postinstall": ""
  },
  "dependencies": {
    "ajv": "^6.5.3",
    "clone": "^2.1.1",
    "concat-stream": "^1.6.0",
    "d3": "^5.7.0",
    "electron-config": "^1.0.0",
    "elkjs": "^0.4.1",
    "glsl-transpiler": "^1.5.9",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
    "keyboardjs": "^2.3.4",
    "lodash": "^4.17.4",
    "osc": "^2.2.0",
    "tar-stream": "^1.5.4",
    "three": "^0.96.0",
    "three.js-fbo": "^1.5.0",
    "tinycolor2": "^1.4.1",
    "typescript": "^3.0.3",
    "url-pattern": "^1.0.3",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-template-es2015-compiler": "^1.6.0",
    "vuex": "^3.0.1",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.7"
  },
  "devDependencies": {
    "cache-loader": "^1.0.3",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "devtron": "^1.1.0",
    "electron": "^2.0.8",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^12.1.1",
    "electron-rebuild": "^1.6.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.9.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.5.0",
    "jest-vue-preprocessor": "^1.0.1",
    "json-loader": "^0.5.4",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.0.0",
    "url-loader": "^1.1.1",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/common/(.+)": "<rootDir>/../common/$1",
      "^@/(.+)": "<rootDir>/src/$1",
      "^chl/(.+)": "<rootDir>/src/js/$1"
    },
    "transform": {
      "^.+\\.vue$": "jest-vue-preprocessor",
      ".*": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest-preamble.js"
  }
}
